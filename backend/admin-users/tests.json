{
  "tests": [
    {
      "name": "OPTIONS request for CORS",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedBody": "",
      "bodyMatcher": "exact"
    },
    {
      "name": "Get users without admin token",
      "method": "GET",
      "path": "/",
      "headers": {},
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Get users with admin token",
      "method": "GET",
      "path": "/",
      "headers": {
        "X-Admin-Token": "admin_authenticated"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": "boolean",
        "users": "array",
        "total": "number"
      },
      "bodyMatcher": "partial"
    }
  ]
}
